{
  "name": "@skillancer/auth",
  "version": "0.0.1",
  "private": true,
  "description": "Shared authentication utilities, hooks, and permission management",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.mjs",
      "require": "./dist/client/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js"
    },
    "./permissions": {
      "types": "./dist/permissions/index.d.ts",
      "import": "./dist/permissions/index.mjs",
      "require": "./dist/permissions/index.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "jose": "^5.2.0"
  },
  "devDependencies": {
    "@skillancer/config": "workspace:*",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "react": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  }
}
