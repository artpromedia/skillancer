{
  "name": "@skillancer/alerting",
  "version": "0.0.0",
  "description": "Alerting with PagerDuty for Skillancer",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./pagerduty": {
      "import": {
        "types": "./dist/pagerduty.d.ts",
        "default": "./dist/pagerduty.js"
      },
      "require": {
        "types": "./dist/pagerduty.d.cts",
        "default": "./dist/pagerduty.cjs"
      }
    },
    "./cloudwatch": {
      "import": {
        "types": "./dist/cloudwatch.d.ts",
        "default": "./dist/cloudwatch.js"
      },
      "require": {
        "types": "./dist/cloudwatch.d.cts",
        "default": "./dist/cloudwatch.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@pagerduty/pdjs": "^2.2.4"
  },
  "devDependencies": {
    "@skillancer/config": "workspace:*",
    "@types/node": "^20.0.0",
    "eslint": "^8.57.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
