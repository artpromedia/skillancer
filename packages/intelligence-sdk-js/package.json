{
  "name": "@skillancer/intelligence-sdk",
  "version": "1.0.0",
  "description": "Official JavaScript/TypeScript SDK for the Skillancer Talent Intelligence API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "skillancer",
    "api",
    "sdk",
    "talent",
    "intelligence",
    "freelance",
    "rates",
    "workforce"
  ],
  "author": "Skillancer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/skillancer/intelligence-sdk-js"
  },
  "homepage": "https://skillancer.com/api-portal/docs",
  "bugs": {
    "url": "https://github.com/skillancer/intelligence-sdk-js/issues"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
