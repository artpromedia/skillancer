{
  "name": "@skillancer/service-client",
  "version": "0.0.0",
  "private": true,
  "description": "Inter-service communication clients for Skillancer microservices",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./auth": {
      "types": "./dist/clients/auth-client.d.ts",
      "import": "./dist/clients/auth-client.js"
    },
    "./market": {
      "types": "./dist/clients/market-client.d.ts",
      "import": "./dist/clients/market-client.js"
    },
    "./skillpod": {
      "types": "./dist/clients/skillpod-client.d.ts",
      "import": "./dist/clients/skillpod-client.js"
    },
    "./cockpit": {
      "types": "./dist/clients/cockpit-client.d.ts",
      "import": "./dist/clients/cockpit-client.js"
    },
    "./billing": {
      "types": "./dist/clients/billing-client.d.ts",
      "import": "./dist/clients/billing-client.js"
    },
    "./notification": {
      "types": "./dist/clients/notification-client.d.ts",
      "import": "./dist/clients/notification-client.js"
    },
    "./queue": {
      "types": "./dist/queue/index.d.ts",
      "import": "./dist/queue/index.js"
    },
    "./events": {
      "types": "./dist/events/index.d.ts",
      "import": "./dist/events/index.js"
    },
    "./saga": {
      "types": "./dist/saga/index.d.ts",
      "import": "./dist/saga/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "bullmq": "^5.65.1",
    "got": "^14.6.5",
    "ioredis": "^5.8.2",
    "meilisearch": "^0.54.0",
    "pino": "^9.14.0"
  },
  "devDependencies": {
    "@skillancer/config": "workspace:*",
    "@types/node": "^20.10.0",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "@skillancer/types": "workspace:*"
  }
}
