{
  "name": "@skillancer/audit-client",
  "version": "0.1.0",
  "description": "Audit logging client for Skillancer services",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./fastify": {
      "import": "./dist/fastify-plugin.js",
      "types": "./dist/fastify-plugin.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "bullmq": "^5.34.8",
    "fastify-plugin": "^4.5.1",
    "ioredis": "^5.4.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@skillancer/config": "workspace:*",
    "@types/node": "^22.10.10",
    "@types/uuid": "^10.0.0",
    "fastify": "^4.29.1",
    "tsup": "^8.3.6",
    "typescript": "~5.4.0"
  },
  "peerDependencies": {
    "fastify": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "fastify": {
      "optional": true
    }
  }
}
