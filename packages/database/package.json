{
  "name": "@skillancer/database",
  "version": "0.0.1",
  "private": true,
  "description": "Prisma schema and database utilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsup src/index.ts --watch --dts",
    "build": "tsup src/index.ts --dts",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:seed": "tsx src/seed.ts",
    "db:studio": "prisma studio",
    "clean": "rm -rf dist node_modules generated"
  },
  "dependencies": {
    "@prisma/client": "^5.9.0"
  },
  "devDependencies": {
    "@skillancer/config": "workspace:*",
    "prisma": "^5.9.0",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
