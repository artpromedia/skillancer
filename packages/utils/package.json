{
  "name": "@skillancer/utils",
  "version": "0.0.0",
  "private": true,
  "description": "Shared utility functions for the Skillancer platform",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dates": {
      "types": "./dist/dates.d.ts",
      "import": "./dist/dates.js",
      "require": "./dist/dates.cjs"
    },
    "./currency": {
      "types": "./dist/currency.d.ts",
      "import": "./dist/currency.js",
      "require": "./dist/currency.cjs"
    },
    "./strings": {
      "types": "./dist/strings.d.ts",
      "import": "./dist/strings.js",
      "require": "./dist/strings.cjs"
    },
    "./validation": {
      "types": "./dist/validation.d.ts",
      "import": "./dist/validation.js",
      "require": "./dist/validation.cjs"
    },
    "./async": {
      "types": "./dist/async.d.ts",
      "import": "./dist/async.js",
      "require": "./dist/async.cjs"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "import": "./dist/errors.js",
      "require": "./dist/errors.cjs"
    },
    "./ids": {
      "types": "./dist/ids.d.ts",
      "import": "./dist/ids.js",
      "require": "./dist/ids.cjs"
    },
    "./objects": {
      "types": "./dist/objects.d.ts",
      "import": "./dist/objects.js",
      "require": "./dist/objects.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup || true",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "date-fns": "^3.3.0",
    "nanoid": "^5.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@skillancer/config": "workspace:*",
    "@types/uuid": "^9.0.0",
    "@vitest/coverage-v8": "^1.6.1",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  }
}
