{
  "name": "@skillancer/shared-api-client",
  "version": "0.0.1",
  "private": true,
  "description": "Shared API client library for Skillancer web applications",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./endpoints": {
      "types": "./dist/endpoints.d.ts",
      "import": "./dist/endpoints.mjs",
      "require": "./dist/endpoints.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules .turbo"
  },
  "dependencies": {
    "@skillancer/types": "workspace:*",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@skillancer/config": "workspace:*",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "zustand": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "zustand": {
      "optional": true
    }
  }
}
