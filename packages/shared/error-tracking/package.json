{
  "name": "@skillancer/error-tracking",
  "version": "0.0.1",
  "description": "Error tracking integration for Skillancer applications using Sentry",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.mjs",
      "require": "./dist/react.js",
      "types": "./dist/react.d.ts"
    },
    "./node": {
      "import": "./dist/node.mjs",
      "require": "./dist/node.js",
      "types": "./dist/node.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@sentry/browser": "^8.0.0",
    "@sentry/node": "^8.0.0",
    "@sentry/react": "^8.0.0"
  },
  "devDependencies": {
    "@skillancer/config": "workspace:*",
    "@skillancer/error-handling": "workspace:*",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "react": "^18.3.1",
    "tsup": "^8.5.1",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
