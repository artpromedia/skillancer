{
  "name": "@skillancer/error-handling",
  "version": "0.0.1",
  "description": "Centralized error handling for Skillancer applications",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./errors": {
      "import": "./dist/errors.mjs",
      "require": "./dist/errors.js",
      "types": "./dist/errors.d.ts"
    },
    "./handlers": {
      "import": "./dist/handlers.mjs",
      "require": "./dist/handlers.js",
      "types": "./dist/handlers.d.ts"
    },
    "./constants": {
      "import": "./dist/constants.mjs",
      "require": "./dist/constants.js",
      "types": "./dist/constants.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rimraf dist"
  },
  "dependencies": {},
  "devDependencies": {
    "@skillancer/config": "workspace:*",
    "@types/node": "^20.14.2",
    "axios": "^1.7.2",
    "tsup": "^8.5.1",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "axios": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "axios": {
      "optional": true
    }
  }
}
